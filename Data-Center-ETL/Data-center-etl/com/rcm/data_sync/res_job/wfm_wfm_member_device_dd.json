{
  "type": "job",
  "steps": [
    {
      "stepType": "mysql",
      "parameter": {
        "column": [
          "id",
          "member_id",
          "phone_number",
          "platform_id",
          "term_sys_version",
          "term_model",
          "term_id",
          "finger_print",
          "network",
          "network_operator",
          "major_app_version",
          "app_version",
          "app_type",
          "p_value",
          "register_channel",
          "source_ip",
          "position_lon",
          "position_lat",
          "add_time",
          "update_time",
          "app_id"
        ],
        "connection": [
          {
            "datasource": "wfm",
            "table": [
              "wfm_member_device"
            ]
          }
        ],
        "where": "(add_time >= '${date} 00:00:00') or (update_time >= '${date} 00:00:00')",
        "splitPk": "id",
        "encoding": "UTF-8"
      },
      "name": "Reader",
      "category": "reader"
    },
    {
      "stepType": "odps",
      "parameter": {
        "partition": "ts=${ts},hour=${hour}",
        "truncate": true,
        "datasource": "dc_ods",
        "column": [
          "*"
        ],
        "table": "wfm_wfm_member_device_dd"
      },
      "name": "Writer",
      "category": "writer"
    }
  ],
  "version": "2.0",
  "order": {
    "hops": [
      {
        "from": "Reader",
        "to": "Writer"
      }
    ]
  },
  "setting": {
    "errorLimit": {
      "record": "0"
    },
    "speed": {
      "throttle": false,
      "concurrent": 3,
      "dmu": 3
    }
  }
}