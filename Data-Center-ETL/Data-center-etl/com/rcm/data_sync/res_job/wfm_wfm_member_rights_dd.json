{
  "type": "job",
  "steps": [
    {
      "stepType": "mysql",
      "parameter": {
        "column": [
          "id",
          "member_id",
          "member_phone",
          "member_level",
          "super_member_status",
          "active_time",
          "expire_time",
          "frozen_time",
          "create_time",
          "update_uid",
          "update_time",
          "del_flag",
          "app_id"
        ],
        "connection": [
          {
            "datasource": "wfm",
            "table": [
              "wfm_member_rights"
            ]
          }
        ],
        "where": "(add_time >= '${date} 00:00:00') or (update_time >= '${date} 00:00:00')",
        "splitPk": "id",
        "encoding": "UTF-8"
      },
      "name": "Reader",
      "category": "reader"
    },
    {
      "stepType": "odps",
      "parameter": {
        "partition": "ts=${ts},hour=${hour}",
        "truncate": true,
        "datasource": "dc_ods",
        "column": [
          "*"
        ],
        "table": "wfm_wfm_member_rights_dd"
      },
      "name": "Writer",
      "category": "writer"
    }
  ],
  "version": "2.0",
  "order": {
    "hops": [
      {
        "from": "Reader",
        "to": "Writer"
      }
    ]
  },
  "setting": {
    "errorLimit": {
      "record": "0"
    },
    "speed": {
      "throttle": false,
      "concurrent": 3,
      "dmu": 3
    }
  }
}