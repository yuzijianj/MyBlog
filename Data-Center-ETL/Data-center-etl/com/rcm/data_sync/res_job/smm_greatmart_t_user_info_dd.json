{
  "type": "job",
  "steps": [
    {
      "stepType": "mysql",
      "parameter": {
        "column": [
          "id",
          "member_id",
          "open_id",
          "union_id",
          "parent_id",
          "name",
          "nick_name",
          "id_number",
          "phone_number",
          "type",
          "team_id",
          "team_name",
          "level",
          "head_flag",
          "head_exist_flag",
          "bind_flag",
          "icon",
          "qr_icon",
          "withdraw_first_time",
          "withdraw_times",
          "del_flag",
          "create_time",
          "bind_time",
          "admire_time",
          "join_time",
          "update_time",
          "effect_time",
          "un_effect_time",
          "zero_startup",
          "invite_code",
          "consultant_time",
          "senior_time",
          "in_kawei",
          "task_status",
          "last_qrcode_time"
        ],
        "connection": [
          {
            "datasource": "smm_greatmart",
            "table": [
              "t_user_info"
            ]
          }
        ],
        "where": "(add_time >= '${date} 00:00:00') or (update_time >= '${date} 00:00:00')",
        "splitPk": "id",
        "encoding": "UTF-8"
      },
      "name": "Reader",
      "category": "reader"
    },
    {
      "stepType": "odps",
      "parameter": {
        "partition": "ts=${ts},hour=${hour}",
        "truncate": true,
        "datasource": "dc_ods",
        "column": [
          "*"
        ],
        "table": "smm_greatmart_t_user_info_dd"
      },
      "name": "Writer",
      "category": "writer"
    }
  ],
  "version": "2.0",
  "order": {
    "hops": [
      {
        "from": "Reader",
        "to": "Writer"
      }
    ]
  },
  "setting": {
    "errorLimit": {
      "record": "0"
    },
    "speed": {
      "throttle": false,
      "concurrent": 3,
      "dmu": 3
    }
  }
}