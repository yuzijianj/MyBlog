{
  "type": "job",
  "steps": [
    {
      "stepType": "mysql",
      "parameter": {
        "column": [
          "id",
          "memberId",
          "receiver_name",
          "defaultFlag",
          "mobile",
          "province_code",
          "province_name",
          "city_code",
          "city_name",
          "district_code",
          "district_name",
          "address",
          "email",
          "zip_code",
          "label",
          "real_name",
          "id_card_number",
          "id_card_valid_flag",
          "create_time",
          "update_time",
          "del_flag"
        ],
        "connection": [
          {
            "datasource": "smm_order",
            "table": [
              "t_user_addressinfo"
            ]
          }
        ],
        "where": "(add_time >= '${date} 00:00:00') or (update_time >= '${date} 00:00:00')",
        "splitPk": "id",
        "encoding": "UTF-8"
      },
      "name": "Reader",
      "category": "reader"
    },
    {
      "stepType": "odps",
      "parameter": {
        "partition": "ts=${ts},hour=${hour}",
        "truncate": true,
        "datasource": "dc_ods",
        "column": [
          "*"
        ],
        "table": "smm_order_t_user_addressinfo_dd"
      },
      "name": "Writer",
      "category": "writer"
    }
  ],
  "version": "2.0",
  "order": {
    "hops": [
      {
        "from": "Reader",
        "to": "Writer"
      }
    ]
  },
  "setting": {
    "errorLimit": {
      "record": "0"
    },
    "speed": {
      "throttle": false,
      "concurrent": 3,
      "dmu": 3
    }
  }
}